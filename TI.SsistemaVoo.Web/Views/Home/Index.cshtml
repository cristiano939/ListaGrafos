    <section class="probootstrap-cover overflow-hidden relative" style="background-image: url('~/images/plane_bg1.jpg');" data-stellar-background-ratio="0.5" id="section-home">
        <div class="overlay"></div>
        <div class="container">
            <div class="row align-items-center">
                <div class="col-md">
                    <h2 class="heading mb-2 display-4 font-light probootstrap-animate">Explore o Brasil com nossos voos</h2>
                    <p class="lead mb-5 probootstrap-animate">Confie em quem tem experiência em Grafos para encontrar o caminho mais rápido até suas ferias</p>
                    <p class="probootstrap-animate">
                       
                    </p>
                </div>
                <div class="col-md probootstrap-animate">
                    <form  method="POST" action="SearchFlights" class="probootstrap-form">
                        <div class="form-group">
                            <div class="row mb-3">
                                <div class="col-md">
                                    <div class="form-group">
                                        <label for="idOrigemDD">Origem</label>
                                        <label for="idOrigemDD" style="width: 100%;">
                                            <select class="js-example-basic-single js-states form-control" id="idOrigemDD" style="width: 100%;">
                                                @{var grafo = (GrafoRotasVoo)ViewData["Grafo"]; }
                                                @foreach (var vertice in grafo.Vertices)
                                                {
                                                    <option value="@vertice.Aeroporto">@vertice.Aeroporto</option>
                                                }

                                            </select>
                                        </label>

                                    </div>
                                </div>
                                <div class="col-md">
                                    <div class="form-group">
                                        <label for="idDestinoDD">Destino</label>
                                        <div class="probootstrap_select-wrap">
                                            <label for="idDestinoDD" style="width: 100%;">
                                                <select class="js-example-basic-single js-states form-control" id="idDestinoDD" style="width: 100%;">
                                                    @foreach (var vertice in grafo.Vertices)
                                                    {
                                                        <option value="@vertice.Aeroporto">@vertice.Aeroporto</option>
                                                    }

                                                </select>
                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- END row -->
                            <div class="row mb-5">
                                <div class="col-md">
                                    <div class="form-group">
                                        <label for="DataIda">Data de ida</label>
                                        <div class="probootstrap-date-wrap">
                                            <span class="icon ion-calendar"></span>
                                            <input type="text" id="probootstrap-date-departure" class="form-control" placeholder="">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md">
                                    <div class="form-group">
                                        <label for="DataChegada">Data de volta</label>
                                        <div class="probootstrap-date-wrap">
                                            <span class="icon ion-calendar"></span>
                                            <input type="text" id="DataChegada" class="form-control" placeholder="">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- END row -->
                            <div class="row">
                                <div class="col-md">
                                    <label for="round" class="mr-5"><input type="radio" id="round" name="direction">  Ida e volta</label>
                                    <label for="oneway"><input type="radio" id="oneway" name="direction">  Apenas ida</label>
                                </div>
                                <div class="col-md">
                                    <input type="submit" value="Ver voos" class="btn btn-primary btn-block" >
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
